{
  "name": "@tobimadehin/playground",
  "version": "1.0.19",
  "description": "Multi-cloud ephemeral VM library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "release:patch": "npm version patch && git push origin main --tags",
    "release:minor": "npm version minor && git push origin main --tags",
    "release:major": "npm version major && git push origin main --tags",
    "prepublishOnly": "npm test && npm run build"
  },
  "keywords": [
    "cloud",
    "vm",
    "ephemeral",
    "aws",
    "azure",
    "gcp",
    "digitalocean",
    "hetzner",
    "oracle",
    "dployr"
  ],
  "author": "Emmanuel Madehin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tobimadehin/playground.git"
  },
  "homepage": "https://tobimadehin.github.io/playground",
  "bugs": {
    "url": "https://github.com/tobimadehin/playground/issues"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/graceful-fs": "^4.1.0",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.0.0",
    "@azure/arm-compute": "^21.0.0",
    "@azure/arm-network": "^33.0.0",
    "@azure/identity": "^4.0.0",
    "@google-cloud/compute": "^4.0.0",
    "js-yaml": "^4.1.0",
    "oci-sdk": "^2.0.0"
  },
  "type": "module"
}
